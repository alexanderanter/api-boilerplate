{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "no-undef": "warn",
    "no-unreachable": "warn",
    "no-unused-vars": "warn",
    "prettier/prettier": "error",
    "func-names": "off",
    "no-param-reassign": "off",
    "no-underscore-dangle": "off",
    "comma-dangle": "warn"
  }
}
